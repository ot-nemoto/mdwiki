<%= content_tag(:div, nil, {:class => 'mdwiki_logo'}) do %>
  <%= render(:partial => "header") %>
<% end %>

<%= content_tag(:div, nil, {:class => 'mdwiki_header'}) do %>
  <%= content_tag(:ul) do %>
    <%= content_tag(:li) do %>
      <%= link_to('Create', '/mdwiki/' + Settings.root_parent + '/new', {:class => 'mdwiki_button'}) %>
    <% end %>
  <% end %>
<% end %>
<%= content_tag(:div, nil, {:id => ('div_' + Settings.root_parent), :class => 'mdwiki_menu'}) do %>
  <%= render(:partial => "list", :object => @summaries) %>
<% end %>
