<%= content_tag(:div, nil) do %>
  <%= render(:partial => "header", :object  => @menu_flg) %>
<% end %>

<%= content_tag(:div, nil, {:class => 'mdwiki_breadcrumb'}) do %>
  <%= render(:partial => 'breadcrumb', :locals => {:breadcrumb_list => @content.breadcrumb_list}) %>
<% end %>

<%= content_tag(:div, nil, {:class => 'mdwiki_show'}) do %>
